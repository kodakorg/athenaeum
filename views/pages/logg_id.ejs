        <div><span class="text-primary">Dato leie: </span>
            <%= row.leie_dato %>
        </div>
        <div><span class="text-primary">Form√•l: </span>
            <%= row.tekst %>
        </div>
        <div><span class="text-primary">Lokaler: </span>
            <%= row.lokaler %>
        </div>
        <div><span class="text-primary">Tilbakemelding: </span>
            <%= row.tilbakemelding %>
        </div>
        <div><span class="text-primary">Ip: </span>
            <%= row.ip %>
        </div>
        <div><span class="text-primary">User-Agent: </span>
            <%= row.user_agent %>
        </div>
        <a href="/logg" class="btn btn-primary">Tilbake</a>
        <a href="/logout" class="btn btn-primary">Logg ut</a>
    </div>
    <% } %>

    <script>
        document.getElementById("brand").textContent = "Logg Id";
    </script>

    <%- include('../partials/footer') -%>

</body>

</html>